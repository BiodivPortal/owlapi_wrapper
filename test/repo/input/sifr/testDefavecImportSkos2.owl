	<!-- ConceptScheme -->

	<!-- http://ontology.irstea.fr/bsv/cropusage/FrenchCropUsage -->

	<owl:NamedIndividual rdf:about="http://ontology.irstea.fr/bsv/cropusage/FrenchCropUsage">
		<rdf:type rdf:resource="&skos;ConceptScheme"/>
		<skos:note xml:lang="fr">Catégories des végétaux cultivés en France en fonction de leurs usages</skos:note>
		<skos:prefLabel xml:lang="fr">Usage des cultures en France</skos:prefLabel>
		<skos:hasTopConcept rdf:resource="http://ontology.irstea.fr/bsv/cropusage/Arboriculture"/>
		<skos:hasTopConcept rdf:resource="http://ontology.irstea.fr/bsv/cropusage/Grandes_cultures"/>
		<skos:hasTopConcept rdf:resource="http://ontology.irstea.fr/bsv/cropusage/Cultures_tropicales"/>
	</owl:NamedIndividual>


	<!-- skos:concept -->

	<!-- http://ontology.irstea.fr/bsv/cropusage/Grandes_cultures -->

	<owl:NamedIndividual rdf:about="http://ontology.irstea.fr/bsv/cropusage/Grandes_cultures">
		<rdf:type rdf:resource="&skos;Concept"/>
		<skos:prefLabel xml:lang="fr">Grande culture</skos:prefLabel>
		<skos:example xml:lang="fr">Céréales, oléagineux, protéagineux et quelques légumes</skos:example>
		<skos:inScheme rdf:resource="http://ontology.irstea.fr/bsv/cropusage/FrenchCropUsage"/>
	</owl:NamedIndividual>

	
	 <!-- http://ontology.irstea.fr/bsv/cropusage/Arboriculture -->

	<owl:NamedIndividual rdf:about="http://ontology.irstea.fr/bsv/cropusage/Arboriculture">
		<rdf:type rdf:resource="&skos;Concept"/>
		<skos:prefLabel xml:lang="fr">Arboriculture</skos:prefLabel>
		<skos:definition xml:lang="fr">Culture des arbres et des arbustes</skos:definition>
		<skos:inScheme rdf:resource="http://ontology.irstea.fr/bsv/cropusage/FrenchCropUsage"/>
		<skos:example xml:lang="fr">Arboriculture, verger, pépinière, sylviculture, forêt</skos:example>
		<rdfs:seeAlso xml:lang="fr">https://fr.wikipedia.org/wiki/Arboriculture</rdfs:seeAlso>
	</owl:NamedIndividual>


   	<!-- http://ontology.irstea.fr/bsv/cropusage/Arboriculture_ornementale -->

	<owl:NamedIndividual rdf:about="http://ontology.irstea.fr/bsv/cropusage/Arboriculture_ornementale">
		<rdf:type rdf:resource="&skos;Concept"/>
		<skos:prefLabel xml:lang="fr">Arboriculture ornementale, végétaux d'ornement</skos:prefLabel>
		<skos:definition xml:lang="fr">Culture des arbres et arbustes d'agrément</skos:definition>
		<skos:broader rdf:resource="http://ontology.irstea.fr/bsv/cropusage/Arboriculture"/>
	</owl:NamedIndividual>



	 <!-- http://ontology.irstea.fr/bsv/cropusage/Cultures_tropicales -->

	<owl:NamedIndividual rdf:about="http://ontology.irstea.fr/bsv/cropusage/Cultures_tropicales">
		<rdf:type rdf:resource="&skos;Concept"/>
		<skos:prefLabel xml:lang="fr">Culture tropicale</skos:prefLabel>
		<skos:note xml:lang="fr">La plupart des techniques agricoles issues des zones tempérées ne sont pas appropriées aux zones tropicales</skos:note>
		<rdfs:seeAlso xml:lang="fr">http://www.larousse.fr/archives/agricole/page/193</rdfs:seeAlso>
		<skos:narrower rdf:resource="http://ontology.irstea.fr/bsv/cropusage/Ananas"/>
		<skos:inScheme rdf:resource="http://ontology.irstea.fr/bsv/cropusage/FrenchCropUsage"/>
	</owl:NamedIndividual>


	  <!-- http://ontology.irstea.fr/bsv/cropusage/Ananas -->

	<owl:NamedIndividual rdf:about="http://ontology.irstea.fr/bsv/cropusage/Ananas">
		<rdf:type rdf:resource="&skos;Concept"/>
		<skos:prefLabel xml:lang="fr">Ananas</skos:prefLabel>
		<skos:definition xml:lang="fr">Plante basse semi-pérenne, cultivée en régions tropicales qui fournit un fruit à la pulpe savoureuse</skos:definition>
		<rdfs:seeAlso xml:lang="fr">http://www.larousse.fr/archives/agricole/page/30</rdfs:seeAlso>
		<skos:historyNote>Christophe Colomb découvrit ce fruit lorsqu'il arriva en Guadeloupe, en 1493</skos:historyNote>
		<skos:broader rdf:resource="http://ontology.irstea.fr/bsv/cropusage/Cultures_tropicales"/>
	</owl:NamedIndividual>


</rdf:RDF>
