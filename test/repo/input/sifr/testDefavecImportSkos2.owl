	<owl:Ontology rdf:about="http://ontology.irstea.fr/bsv/cropusage/">
		<dcterms:title>Classification des plantes en fonction de leur usage en France</dcterms:title>
		<rdfs:comment xml:lang="fr">Sources : Larousse agricole, wikipedia</rdfs:comment>
		<dc:creator>Alfred de Musset</dc:creator>
		<dc:description>Ce vocabulaire répertorie les différents types de culture en France</dc:description>
		<dc:date>2015-07-16</dc:date>
		<dcterms:abstract>Cette classification sera utilisée pour organiser les Bulletins de Santé du Végétal (BSV)</dcterms:abstract>
		<dcterms:publisher>http://ontology.irstea.fr/bsv/cropusage#IRSTEA</dcterms:publisher>
		<owl:versionIRI rdf:resource="http://ontology.irstea.fr/bsv/cropusage/2015/12/07"/>
